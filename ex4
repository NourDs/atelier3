#include <iostream>
using namespace std;
    class mere{
        public:

        void display();

    };
    void mere::display(){
        cout<<"Mere ";
    }
    class fille : public mere{
        public:
        void display();
    };
    void fille::display(){
        cout<<"Fille ";
    }
int main()
{
    fille f1;
    f1.display();
    return 0;
}
